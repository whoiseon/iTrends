{
  "name": "native",
  "main": "expo-router/entry",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "clean": "rm -rf .turbo .expo .expo-shared ios android node_modules",
    "ios:build:local": "expo run:ios",
    "android:build:local": "expo run:android",
    "detox:android:debug:build": "detox build -c android.debug",
    "detox:android:debug:test": "detox test -c android.debug",
    "detox:android:release:build": "detox build -c android.release",
    "detox:android:release:test": "detox test -c android.release",
    "detox:ios:debug:build": "detox build -c ios.debug",
    "detox:ios:debug:test": "DETOX=true detox test -c ios.debug",
    "detox:ios:release:build": "detox build -c ios.release",
    "detox:ios:release:test": "DETOX=true detox test -c ios.release"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@bacons/expo-router-top-tabs": "^1.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@itrends/ui": "*",
    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.43",
    "expo-dev-client": "^5.0.20",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-keyboard-controller": "^1.17.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.11.2",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-web": "~0.19.13",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@itrends/eslint-config": "*",
    "@types/react": "~18.3.12",
    "detox": "^20.36.5",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
